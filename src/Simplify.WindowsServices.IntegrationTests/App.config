			</dependentAssembly>
			<dependentAssembly>
				<assemblyIdentity name="SimpleInjector.Extensions.LifetimeScoping" publicKeyToken="984cb50dea722e99" culture="neutral" />
				<bindingRedirect oldVersion="0.0.0.0-3.2.0.0" newVersion="3.2.0.0" />
			</dependentAssembly>
